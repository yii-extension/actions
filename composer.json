{
    "name": "yii-extension/actions",
    "type": "library",
    "description": "Aliases",
    "keywords": [
        "yii",
        "actions"
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "infection/infection": "^0.17",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.15"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Extension\\Action\\": "src"
        }
    },    
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Actions\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
